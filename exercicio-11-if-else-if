#include <stdio.h>

int main()

{
    float numero1,numero2;
    
    printf("qual numero é maior?\n");
    printf("digite o primeiro numero:\n");
    scanf("%f",&numero1);
    
    printf("digite o segudo numero:\n");
    scanf("%f",&numero2);
    
     if(numero1 > numero2){
     printf("primeiro numero é maior!\n");
     }
     else{
     printf("segundo numero é maior!\n");
     }
    return 0;
}
